    "collectCoverageFrom":[
      "src/**/*.{js,jsx}",
      "!collectCoverage/**/*.{js,jsx}"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "devDependencies": {
    "@babel/preset-env": "^7.22.20",
    "babel-jest": "^29.7.0"
  },
  "dependencies": {
    "jest": "^29.7.0"
  }
}
